@model IEnumerable<Give_Pro.Models.ApplyForJob>

@{
    ViewBag.Title = "الوظائف المتقدم إليها من طرفكم";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<h2>جميع الوظائف المتقدم إليها من طرفكم</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Jobs.JobName)
        </th>

        <th>
          نص الرسالة  @*@Html.DisplayNameFor(model => model.Message)*@
        </th>
        <th>
          تاريخ الارسال  @*@Html.DisplayNameFor(model => model.ApplyDate)*@
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Jobs.JobName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Message)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ApplyDate)
            </td>

            <td>
                @Html.ActionLink("تعديل", "Edit", new { id = item.Id }) |
             
                @Html.ActionLink("حذف", "Delete", new { id = item.Id })
            </td>

        </tr>
    }

</table>
