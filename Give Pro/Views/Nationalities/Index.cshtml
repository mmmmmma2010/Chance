@model IEnumerable<Give_Pro.Models.Nationalities>

@{
    ViewBag.Title = "قائمة الجنسيات";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}

<h2>قائمة الجنسيات</h2>


<p>
    @Html.ActionLink("إنشاء جديد", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NationalitiesName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NationalitiesName)
        </td>
        <td>
            @Html.ActionLink("تعديل", "Edit", new { id = item.Id }) |
            @Html.ActionLink("تفاصيل", "Details", new { id = item.Id }) |
            @Html.ActionLink("حذف", "Delete", new { id = item.Id })
        </td>

    </tr>
    }

</table>
